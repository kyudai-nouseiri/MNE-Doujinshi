{
  "bib": {
    "nameNumber": 3,
    "do": false,
    "nameSep": ",",
    "script": "[author]([year]);[title];[journal];[number](pp[pages]);[publisher]"
  },
  "writer": [
    "--toc",
    "--toc-depth=3",
    "-T MNE同人誌",
    "--indented-code-classes=python,bash"
  ],
  "format": "pdf",
  "markdownextension": [
    "markdown",
    "hard_line_breaks"
  ],
  "latexengine": [
    "--pdf-engine=lualatex"
  ],
  "latex": [
    "documentclass=ltjarticle",
    "geometry:left=3cm",
    "geometry:right=1cm",
    "geometry:twoside",
    "CJKmainfont=IPAexGothic",
    "lang=en-US"
  ],
  "latexpackage": [
    "--listings",
    "--template eisvogel.tex"
  ],
  "sep": "\t"
}
